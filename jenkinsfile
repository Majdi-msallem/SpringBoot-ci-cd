properties([pipelineTriggers([githubPush()])])
pipeline{
    agent any 
    
   
    
        stages {
            stage ('Hello') {
                steps {
                    echo 'Hello World '
                }
            }
            stage('Checkout GIT ') {
                steps {
                    echo 'Pulling ...';
                    git branch : 'master',
                    url : 'https://github.com/Majdi-msallem/Spring-test.git';
                }
            }
            
        }
}
