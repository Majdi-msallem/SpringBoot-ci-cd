properties([pipelineTriggers([githubPush()])])
pipeline{
    agent any 

        stages {
            stage ('Hello') {
                steps {
                    echo 'Hello World ngrok '
                }
            }
            stage('Checkout GIT ') {
                steps {
                    echo 'Pulling ...';
                    git branch : 'master',
                    url : 'https://github.com/Majdi-msallem/Spring-test.git';
                }
            }
            
            stage('MVN CLEAN'){
            steps {
                sh 'mvn clean'
            }
        }

             stage('MVN INSTALL') {
            steps {
                sh 'mvn -Dplugin=install help:describe'
            }
        }
         
        
        }
}
